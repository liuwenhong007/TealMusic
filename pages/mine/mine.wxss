.container {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 100%;
  height: 500rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: top center;
  background-size: cover;
}

.login-con {
  position: absolute;
  top: 40rpx;
  right: 40rpx;
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  border: 1px solid #fff;
  text-align: center;
  border-radius: 4rpx;
}

.login_text {
  color: #fff;
  font-size: 10px;
}

.login_icon {
  width: 25rpx;
  height: 25rpx;
  vertical-align: middle;
  color: #fff;
  margin-right: 6rpx;
}

.avator {
  width: 200rpx;
  height: 200rpx;
  border-radius: 100%;
  background-color: #fff;
  margin-top: 80rpx;
}

.username {
  display: flex;
  justify-content: flex-end;
  color: #fff;
  font-size: 18px;
  font-weight: 400;
  margin-top: 20rpx;
}
